<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        tools:context=".ui.frgment_nave.ProfileFragment">

        <View
            android:id="@+id/view4"
            android:layout_width="400dp"
            android:layout_height="2.2dp"
            android:layout_marginTop="12dp"
            android:background="@drawable/line_space"
            app:layout_constraintEnd_toEndOf="@+id/e_name"
            app:layout_constraintStart_toStartOf="@+id/e_name"
            app:layout_constraintTop_toBottomOf="@+id/e_name" />

        <View
            android:id="@+id/view5"
            android:layout_width="400dp"
            android:layout_height="2.2dp"
            android:layout_marginTop="12dp"
            android:background="@drawable/line_space"
            app:layout_constraintEnd_toEndOf="@+id/e_age"
            app:layout_constraintStart_toStartOf="@+id/e_age"
            app:layout_constraintTop_toBottomOf="@+id/e_age" />

        <View
            android:id="@+id/view6"
            android:layout_width="400dp"
            android:layout_height="2.2dp"
            android:layout_marginTop="12dp"
            android:background="@drawable/line_space"
            app:layout_constraintEnd_toEndOf="@+id/e_weight"
            app:layout_constraintHorizontal_bias="0.486"
            app:layout_constraintStart_toStartOf="@+id/e_weight"
            app:layout_constraintTop_toBottomOf="@+id/e_weight" />

        <View
            android:id="@+id/view8"
            android:layout_width="400dp"
            android:layout_height="2.2dp"
            android:layout_marginTop="12dp"
            android:background="@drawable/line_space"
            app:layout_constraintEnd_toEndOf="@+id/e_height"
            app:layout_constraintHorizontal_bias="0.486"
            app:layout_constraintStart_toStartOf="@+id/e_height"
            app:layout_constraintTop_toBottomOf="@+id/e_height" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout5"
            android:layout_width="match_parent"
            android:layout_height="160dp"
            android:background="@drawable/custom_card_search"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/text_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:drawableRight="@drawable/baseline_waving_hand_24"
                android:drawablePadding="20dp"
                android:fontFamily="@font/akaya_telivigala"
                android:gravity="center"
                android:hint="Ahmed"
                android:textColor="@color/white"
                android:textSize="26sp"
                app:layout_constraintBottom_toTopOf="@+id/imageView2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="105dp"
                android:layout_height="82dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/text_name"
                app:layout_constraintStart_toStartOf="@+id/text_name"
                app:layout_constraintTop_toBottomOf="@+id/text_name"
                app:srcCompat="@drawable/diet" />


        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/e_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="-2dp"
            android:layout_marginEnd="32dp"
            android:fontFamily="@font/poppins_regular"
            android:hint="name"
            android:textColor="@color/ic_launcher_background"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView9"
            app:layout_constraintTop_toBottomOf="@+id/textView9" />

        <TextView
            android:id="@+id/e_age"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="-2dp"
            android:fontFamily="@font/poppins_regular"
            android:hint="age"
            android:textColor="@color/ic_launcher_background"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="@+id/e_name"
            app:layout_constraintStart_toStartOf="@+id/textView10"
            app:layout_constraintTop_toBottomOf="@+id/textView10" />

        <TextView
            android:id="@+id/e_weight"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="-2dp"
            android:fontFamily="@font/poppins_regular"
            android:hint="weight"
            android:textColor="@color/ic_launcher_background"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="@+id/e_age"
            app:layout_constraintStart_toStartOf="@+id/textView11"
            app:layout_constraintTop_toBottomOf="@+id/textView11" />

        <TextView
            android:id="@+id/e_email"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="-2dp"
            android:fontFamily="@font/poppins_regular"
            android:hint="email"
            android:textColor="@color/white"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="@+id/e_height"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView13"
            app:layout_constraintTop_toBottomOf="@+id/textView13" />

        <TextView
            android:id="@+id/e_height"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="-2dp"
            android:fontFamily="@font/poppins_regular"
            android:hint="height"
            android:textColor="@color/ic_launcher_background"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="@+id/e_weight"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView12"
            app:layout_constraintTop_toBottomOf="@+id/textView12" />


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn_edit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:background="@drawable/btn_login_background"
            android:drawableLeft="@drawable/baseline_edit_24"
            android:drawablePadding="16dp"
            android:letterSpacing=".15"
            android:padding="8dp"
            android:paddingEnd="16dp"
            android:textColor="#FFFFFF"
            android:textSize="20dp"
            android:text="Edit"
            app:layout_constraintEnd_toEndOf="@+id/e_email"
            app:layout_constraintStart_toStartOf="@+id/e_email"
            app:layout_constraintTop_toBottomOf="@+id/e_email" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="24dp"
            android:text="User name"
            android:textColor="@color/disable_bottom_navigation_bar"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout5" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="Age"
            android:textColor="@color/disable_bottom_navigation_bar"
            android:textSize="16dp"
            app:layout_constraintStart_toStartOf="@+id/e_name"
            app:layout_constraintTop_toBottomOf="@+id/view4" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="Weight"
            android:textColor="@color/disable_bottom_navigation_bar"
            android:textSize="16dp"
            app:layout_constraintStart_toStartOf="@+id/e_age"
            app:layout_constraintTop_toBottomOf="@+id/view5" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="Height"
            android:textColor="@color/disable_bottom_navigation_bar"
            app:layout_constraintStart_toStartOf="@+id/e_weight"
            app:layout_constraintTop_toBottomOf="@+id/view6" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="Email"
            android:textColor="@color/white"
            app:layout_constraintStart_toStartOf="@+id/e_height"
            app:layout_constraintTop_toBottomOf="@+id/view8" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>